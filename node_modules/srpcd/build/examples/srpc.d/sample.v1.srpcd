package auth

// import './sample.srpcd'
// import './sample.d.srpcd'

data FileMetadata {
    name: string
    directory: boolean
    path: string
}

interceptor AuthUserInterceptor()

@AuthUserInterceptor()
class File {
    data FileMetadata

    rename(name: string): boolean
    remove(): boolean
    move(source: string, dest: string): boolean

    @AuthUserInterceptor()
    files(): File[]
}

service class DriveService {
    rootFolder(): File
}
